<div class="row center">
    <h1>All <strong><i>Actions</i></strong> from settings bill</h1>

    <table class="cen">
        <tr>
            <th>Type</th>
            <th>Cost</th>
            <th>When</th>
        </tr>

        {{#each act}}
            <tr>
                <td>{{type}}</td>
                <td>{{combinedCost}}</td>
                <td>{{newTime}}</td>
            </tr>
        {{/each}}

    </table>

</div>